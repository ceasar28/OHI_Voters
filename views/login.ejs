                        <h2>Log In</h2>
                        <!-- <form action="/auth/login" method="post" id="logForm"> -->
                        <form id='signUp'>
                            <label>Email</label>
                            <input type="email" class="text" name="username" id="username" required="" autofocus>
                            <br>
                            <label>Password</label>
                            <input type="password" class="password" name="password" id="password" required="">
                            <a href="#">Forgot password?</a>
                            <br><br>
                            <button class="btn" type="submit" id="submitBtn">Log In</button>
                            <!-- <button class="btn" type="submit">Log In</button> -->
                        </form>

                        <p class="account">Don't have an account? <a href="/register">Register</a></p>
                    </div>

                </div>
